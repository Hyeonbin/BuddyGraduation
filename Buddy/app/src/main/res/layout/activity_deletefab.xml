<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/Buddy_Graphfab_Layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="20dp">

    <TextView
        android:id="@+id/Buddy_Graphfab_Bar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="평균 그래프"
        android:textSize="18dp"
        android:textColor="#ffffff"
        android:background="#00000000"
        android:layout_alignParentRight="true"
        android:padding="15dp"
        android:layout_above="@+id/Buddy_Graphfab_Morning"
        android:layout_marginBottom="20dp"/>

    <TextView
        android:id="@+id/Buddy_Graphfab_Morning"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="아침 그래프"
        android:textSize="18dp"
        android:textColor="#ffffff"
        android:background="#00000000"
        android:layout_alignParentRight="true"
        android:padding="15dp"
        android:layout_above="@+id/Buddy_Graphfab_Noon"
        android:layout_marginBottom="20dp"/>

    <TextView
        android:id="@+id/Buddy_Graphfab_Noon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="점심 그래프"
        android:textSize="18dp"
        android:textColor="#ffffff"
        android:background="#00000000"
        android:layout_alignParentRight="true"
        android:padding="15dp"
        android:layout_above="@+id/Buddy_Graphfab_Night"
        android:layout_marginBottom="20dp"/>

    <TextView
        android:id="@+id/Buddy_Graphfab_Night"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="저녁 그래프"
        android:textSize="18dp"
        android:textColor="#ffffff"
        android:background="#00000000"
        android:layout_alignParentRight="true"
        android:padding="15dp"
        android:layout_above="@+id/Buddy_Graphfab_Sleep"
        android:layout_marginBottom="20dp"/>

    <TextView
        android:id="@+id/Buddy_Graphfab_Sleep"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="취침 그래프"
        android:textSize="18dp"
        android:textColor="#ffffff"
        android:background="#00000000"
        android:padding="15dp"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="70dp"
        />

</RelativeLayout>