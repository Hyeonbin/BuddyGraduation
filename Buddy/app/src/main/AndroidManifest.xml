<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.seo.buddy">

    <uses-permission android:name="android.permission.WAKE_LOCK"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.VIBRATE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"/>
        <activity android:name=".AddlogActivity"/>
        <activity android:name=".DeleteActivity"/>
        <activity android:name=".NewProfileActivity"/>
        <activity android:name=".ProfileActivity"/>
        <activity android:name=".SettingActivity"/>
        <activity android:name=".SearchDetailActivity"/>
        <activity android:name=".ModifyActivity"/>
        <activity android:name=".FabActivity"
            android:theme="@style/Theme.AppCompat.Translucent"/>
        <activity android:name=".GraphFabActivity"
            android:theme="@style/Theme.AppCompat.Translucent"/>
        <receiver android:name=".AlarmReceiver" />
    </application>

</manifest>